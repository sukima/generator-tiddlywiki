{
  "name": "my-tiddly-wiki",
  "version": "0.0.0",
  "description": "a non-linear personal web notebook",
  "private": true,
  "scripts": {
    <% if (deployCommand) { %>
    "deploy": "npm run build && <%= deployCommand %>",
    <% } %>
    "start": "tiddlywiki wiki --server",
    "build": "tiddlywiki wiki-dist --build index static externalimages",
    "clean": "rm -rf wiki-dist/output"
  },
  "dependencies": {},
  "devDependencies": {
    "tiddlywiki": "^5.1.8"
  }
}
